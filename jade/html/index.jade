doctype html
html
  head
    meta(charset='utf-8')
    title App
    link(rel='stylesheet', href='/css/app.css')
    link(rel='shortcut icon', type='image/ico', href='/favicon.ico')
  body
    header.cd-header(role='banner')
      h1.cd-logo Grupo Super 8
      .cd-primary-nav-trigger
        input#primaryNavTrigger.cd-ckbox(type='checkbox', name='menuTrigger')
        span.cd-menu-text Menu
        span.cd-menu-icon

    nav
      ul#primaryNav.cd-primary-nav
        li.cd-label Sobre o Grupo Super 8
        li
          a(href='#time') O Time
        li
          a(href='#time') Serviços

    section.cd-intro
      .cd-intro__layer.cd-intro__layer--back
        h2.cd-title Back
      .cd-intro__layer.cd-intro__layer--base
        h2.cd-title Grupo Super 8

